<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.shubham.ubiquitousparking.MainActivity">

    <android.support.design.widget.FloatingActionButton
        android:layout_width="94dp"
        android:layout_height="94dp"
        android:clickable="true"
        app:fabSize="mini"
        app:srcCompat="@color/colorAccent"
        android:id="@+id/floatingActionButton2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:onClick="search" />

    <TextView
        android:text="SEARCH"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView"
        android:elevation="50dp"
        app:layout_constraintRight_toLeftOf="@+id/floatingActionButton2"
        app:layout_constraintLeft_toRightOf="@+id/floatingActionButton2"
        app:layout_constraintBottom_toTopOf="@+id/floatingActionButton2"
        app:layout_constraintTop_toBottomOf="@+id/floatingActionButton2"
        android:textColorLink="?attr/actionMenuTextColor"
        android:textColor="@android:color/black" />
</android.support.constraint.ConstraintLayout>
